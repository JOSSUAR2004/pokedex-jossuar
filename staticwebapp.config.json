{
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    },
    "404": {
      "statusCode": 200,
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Content-Security-Policy": "default-src 'self' https://pokeapi.co; script-src 'self' 'unsafe-inline'; img-src 'self' data: https://*.githubusercontent.com"

  },
  "mimeTypes": {
    ".json": "application/json",
    ".webmanifest": "application/manifest+json"
  }
}
